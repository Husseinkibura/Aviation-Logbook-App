{
  "expo": {
    "name": "pilot-logbook",
    "slug": "pilot-logbook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#000000",
      "image": "./assets/images/splash.png"
    },
    "plugins": [
      "expo-router"
    ],
    "android": {
      "package": "com.hussein_kibura.pilotlogbook",
      "adaptiveIcon": { 
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9597974b-fe81-44bd-8609-03c2ddcc775f"
      }
    }
  }
}